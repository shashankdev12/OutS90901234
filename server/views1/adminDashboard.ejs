    <script type="text/javascript">
   
    anychart.onDocumentLoad(function () {
      
      // create pie chart with passed data
      var chart = anychart.pie3d([
       
        ['Junior Users', $('#juniorcount').val()],
        ['Teen Users', $('#teenCount').val()],
        ['Adult Users', $('#adultCount').val()],
      
      ]);

      var chart2 = anychart.pie3d([
        ['Licences', $('#licences').val()],
        ['Registered Users', $('#userCount').val()],
       
      
      ]);
      chart.overlapMode('allow-overlap');
      chart2.overlapMode('allow-overlap');
      var margin = chart.margin();
      var margin2 = chart2.margin();

      
      margin.top(0);
      margin2.top(0);
      // set chart title text settings
      chart
        // .title('Population in The Shire')
        // set chart radius
        .radius('50%');
        chart2.radius('50%');
    
      // set container id for the chart
      chart.container('container');
      chart2.container('container_2');
      // initiate chart drawing
      chart.draw();
      chart2.draw();
      chart.autoRedraw(false);
      
    });

    </script>
